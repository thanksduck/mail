{
  "name": "oas-api",
  "version": "0.0.0.24.9",
  "description": "One Alias Service, Forward to your own main address",
  "main": "dist/server.js",
  "type": "module",
  "files": ["dist"],
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --env-file dev.env server.js",
    "build": "bunchee server.js -o dist/server.js && node scripts/prepare-production.js",
    "prebuild": "rm -rf dist"
  },
  "keywords": [
    "mail",
    "forward",
    "oas.20032003.xyz"
  ],
  "author": "Shivam Shukla",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.5",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.5.4",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.14",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "bunchee": "^5.5.0",
    "nodemon": "^3.1.4"
  },
  "bunchee": {
    "entries": [
      "server.js"
    ]
  }
}